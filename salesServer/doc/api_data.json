l"      }    ],    "parameter": {      "fields": {        "Parameter": [          {            "group": "Parameter",            "type": "String",            "optional": false,            "field": "username",            "description": "<p>filter salesinfo by username(following user)</p>"          },          {            "group": "Parameter",            "type": "String",            "optional": false,            "field": "category",            "description": "<p>filter salesinfo by category,if null, return all categories</p>"          }        ]      }    },    "success": {      "fields": {        "Success 200": [          {            "group": "Success 200",            "type": "Object",            "optional": false,            "field": "postinfo",            "description": "<p>sales information.</p>"          },          {            "group": "Success 200",            "type": "Number",            "optional": false,            "field": "postinfo.id",            "description": "<p>salesinfo id.</p>"          },          {            "group": "Success 200",            "type": "String",            "optional": false,            "field": "postinfo.postUser",            "description": "<p>user who posted the saleinfo.</p>"          },          {            "group": "Success 200",            "type": "String",            "optional": false,            "field": "postinfo.taggedUser",            "description": "<p>user who tagged by this saleinfo.</p>"          },          {            "group": "Success 200",            "type": "String",            "optional": false,            "field": "postinfo.created",            "description": "<p>salesinfo created date.</p>"          },          {            "group": "Success 200",            "type": "String",            "optional": false,            "field": "postinfo.category",            "description": "<p>salesinfo's category.</p>"          },          {            "group": "Success 200",            "type": "Number",            "optional": false,            "field": "postinfo.price_before",            "description": "<p>sales item's before price</p>"          },          {            "group": "Success 200",            "type": "String",            "optional": false,            "field": "postinfo.sale_discount",            "description": "<p>sale discount.</p>"          },          {            "group": "Success 200",            "type": "String",            "optional": false,            "field": "postinfo.shop",            "description": "<p>sales shop.</p>"          },          {            "group": "Success 200",            "type": "String",            "optional": false,            "field": "postinfo.description",            "description": "<p>more details about saleinfo.</p>"          },          {            "group": "Success 200",            "type": "String",            "optional": false,            "field": "postinfo.imagePath",            "description": "<p>salesinfo photo url.</p>"          }        ]      },      "examples": [        {          "title": "Success-Response:",          "content": "HTTP/1.1 200 OK\n{\n    \"category\": \"electronics\",\n      \"created\": \"2016-02-16\",\n      \"description\": \"best chance for whom wants to buy a new PS4\",\n       \"id\": 2,\n       \"imagePath\": \"/Users/danpan/Documents/eclipse/testApp/shareswebservice/image/1.jpeg\",\n       \"postUser\": \"Alice\",\n       \"price_before\": 4000,\n       \"sale_discount\": \"10%\",\n       \"shop\": \"webhallen\",\n       \"taggedUser\": \"dan\"\n}",          "type": "json"        }      ]    },    "error": {      "examples": [        {          "title": "Error-Response:",          "content": "HTTP/1.1 500 Internal Server Error",          "type": "json"        }      ]    },    "version": "0.0.0",    "filename": "shareswebservice/src/main/java/web/shares/resources/PostInfoResource.